GET		/etikett/download.json	controllers.Application.asJson(url : String?=null)
GET		/etikett/context.json	controllers.Application.asJsonLdContext()
GET		/etikett/upload		    controllers.Application.upload()
POST	/etikett               controllers.Application.addSkosData()
GET     /public/*file        	controllers.Assets.at(path="/public", file)
