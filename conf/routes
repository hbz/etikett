GET	    /					controllers.Application.getColumn(url : String?=null,column:String?=null)
GET		/download.json		controllers.Application.asJson(url : String?=null)
GET		/context.json		controllers.Application.asJsonLdContext()
GET		/upload		    	controllers.Application.upload()
POST	/                	controllers.Application.addSkosData()
GET     /public/*file       controllers.Assets.at(path="/public", file)
